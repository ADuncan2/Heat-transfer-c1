	pages = {7821--7826},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\adunc\\Zotero\\storage\\4JILWDQQ\\7260882.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\adunc\\Zotero\\storage\\I7EZHR7U\\Li et al. - 2015 - Lower-limb driven energy harvesting backpack Desi.pdf:application/pdf},
