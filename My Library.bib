	issn = {0043-1354},
	shorttitle = {Impacts of microalgae pre-treatments for improved anaerobic digestion},
	url = {http://www.sciencedirect.com/science/article/pii/S0043135414005363},
	doi = {10.1016/j.watres.2014.07.040},
	abstract = {Anaerobic digestion (AD) of microalgae is primarily inhibited by the chemical composition of their cell walls containing biopolymers able to resist bacterial degradation. Adoption of pre-treatments such as thermal, thermal hydrolysis, ultrasound and enzymatic hydrolysis have the potential to remove these inhibitory compounds and enhance biogas yields by degrading the cell wall, and releasing the intracellular algogenic organic matter (AOM). This work investigated the effect of four pre-treatments on three microalgae species, and their impact on the quantity of soluble biomass released in the media and thus on the digestion process yields. The analysis of the composition of the soluble COD released and of the TEM images of the cells showed two main degradation actions associated with the processes: (1) cell wall damage with the release of intracellular AOM (thermal, thermal hydrolysis and ultrasound) and (2) degradation of the cell wall constituents with the release of intracellular AOM and the solubilisation of the cell wall biopolymers (enzymatic hydrolysis). As a result of this, enzymatic hydrolysis showed the greatest biogas yield increments ({\textgreater}270\%) followed by thermal hydrolysis (60–100\%) and ultrasounds (30–60\%).},
	urldate = {2018-12-16},
	journal = {Water Research},
	author = {Ometto, Francesco and Quiroga, Gerardo and Pšenička, Pavel and Whitton, Rachel and Jefferson, Bruce and Villa, Raffaella},
	month = nov,
	year = {2014},
	keywords = {Anaerobic digestion, Cell wall degradation, Energy balance, Microalgae pre-treatment},
	pages = {350--361},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\adunc\\Zotero\\storage\\W7X4H4D8\\Ometto et al. - 2014 - Impacts of microalgae pre-treatments for improved .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\adunc\\Zotero\\storage\\Q75A55HI\\S0043135414005363.html:text/html},
}

