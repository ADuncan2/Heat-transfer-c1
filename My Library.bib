	keywords = {Anaerobic digestion, Biogas, Macroalgae, Methane, Microalgae, Pretreatment},
	pages = {961--972},
	file = {ScienceDirect Snapshot:C\:\\Users\\adunc\\Zotero\\storage\\VPV8CVIG\\S1364032114009915.html:text/html},
}

